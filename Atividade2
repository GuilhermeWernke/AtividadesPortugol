programa {
  inclua biblioteca Texto
  funcao inicio() {
    
    cadeia letra
    cadeia letra_pronta
    

    escreva("Informe uma letra ")
    leia(letra) 
    cadeia letra_pronta = Texto cadeia caixa_baixa(cadeia letra)
// não sei o que estou errando, mil desculpas, mas tentei de tudo
    se(cadeia letra_pronta=="a" ou cadeia letra_pronta=="e" ou cadeia letra_pronta=="i" ou cadeia letra_pronta=="o" ou cadeia letra_pronta=="u"){
      escreva("Vogal!")
    }
    senao{
      escreva("Consoate!!!!")

    }
  }
}

