programa {
  inclua biblioteca Texto
  funcao inicio() {
    
   inteiro a,b,c

    escreva("Informe um número ")
    leia(a) 
    escreva("Informe um segundo número ")
    leia(b)
    escreva("Informe um terceiro número ")
    leia(c)
    se(a>b e a>b){
      se(c>b){
      	escreva(a, c, b)
 		}
  	 senao{
  	 	escreva(a, b, c)
  	 	}
    }
	senao se(b>a e b>c){
		se(a>c){
     		escreva(b, a, c )
		       }
		senao{
			escreva(b, c, a)
			}
    }
     senao{
     	se(a>b){
     		escreva(c, a, b)
     	}
     	senao{
     		escreva(c, b, a)
     	}
   		}
  }
}

