programa {
  inclua biblioteca Texto
  funcao inicio() {
    
   inteiro a, b

    escreva("Informe uma velocidade: ")
    leia(a) 
    
    se(a<=88){
    	//radares tem tolerancias de 10% SE NÃO ME ENGANO, adicionei por achar legal
    		escreva("Velocidade na média ")
    }
	senao{
		b = (a-88)*7
		escreva("ACIMA DA VELOCIDADE!!!!!!!!!!! \nValor da multa será de: ", b)
   		}
  }
}

