    		 se(ano%100 ==0){
    	 		 se(ano%400 ==0){
    	 		 	escreva("Seu ano é bissesto ")
    			}
    			senao{
    		escreva("Seu ano não é bissesto ")
    		}
    		  }
    		  senao{
    		escreva("Seu ano não é bissesto ")
    		}
    	}
    	senao{
    		escreva("Seu ano não é bissesto ")
    		} 
  //não sei o que eu errei, segui álógica que o senhor informou
  }
}


