programa {
  inclua biblioteca Matematica
  funcao inicio() {
    
    real a = 0
    real b = 0
    real c = 0


    escreva("Qual o tempo que você vai ficar com o carro? ")
    leia(a)
    escreva("Quantos KM você rodou com o carro? ")
    leia(b)
    c = b * 60 + a * 0.15

    escreva("O preço a pagar equivale á: ", c)
  }
}

