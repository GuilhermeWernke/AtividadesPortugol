1_
programa {
  funcao inicio() {
    real Tamanho
    inteiro L
    real P
    escreva("Qual o tamanho a ser pintado (metro quadrado) ")
    leia(Tamanho)
    L = Tamanho / (18 * 3)
    P = L * 480
    escreva("Irá necessitar de " L " latas e gastará: " P)

  }
}

2_
programa {
  inclua biblioteca Matematica
  funcao inicio() {
    
    real a = 0
    real b = 0
    real c = 0
    real delta = 0
    real x = 0
    real y = 0

    escreva("Qual o valor de A? ")
    leia(a)
    escreva("Qual o valor de B? ")
    leia(b)
    escreva("Qual o valor de C? ")
    leia(c)
    delta = b*b-4*a*c
    escreva("Delta equivale á: ", delta)
    delta = Matematica.raiz(delta)
    x = (b*-1 + delta)/2*a
    escreva("X1 equivale á: ", x )
    y = (b*-1 - delta)/2*a
    escreva("X2 equivale á: ", y)

  }
}

3_
programa {
  inclua biblioteca Calendario





  
  funcao inicio() {
    
   inteiro ano, mes, dia, ano_atual,mes_atual, dia_atual, x = 0, y = 0, z
   // meu p ão termina de rodar o programa, não sei pq, 

    escreva("Informe o ano que você naceu (Formato 2000): ")
    leia(ano) 
    escreva("Informe o mês que você naceu (Formato 10): ")
    leia(mes)
    escreva("Informe o dia que você naceu (Formato 09): ")
    leia(dia)
    
    ano_atual = Calendario.ano_atual()
    ano = ano_atual-ano
    mes_atual = ano * 12
    faca{
    		y++
    	} enquanto(y!=mes_atual)
    	mes_atual = mes_atual - y
    	dia_atual = mes_atual*30
    	faca{
    		x++
    	} enquanto(x!=dia_atual)
    	dia_atual = dia_atual * 24 * 60 * 60 
    	escreva("Seus segundos são: ", dia_atual)
  }
}

